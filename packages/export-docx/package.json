{
  "name": "@docen/export-docx",
  "version": "0.0.9",
  "description": "A powerful TipTap/ProseMirror extension that converts editor content to Microsoft Word DOCX format",
  "keywords": [
    "converter",
    "docen",
    "document",
    "docx",
    "editor",
    "export",
    "microsoft-word",
    "office",
    "prosemirror",
    "rich-text",
    "tiptap",
    "typescript",
    "word",
    "wysiwyg"
  ],
  "homepage": "https://github.com/DemoMacro/docen#readme",
  "bugs": {
    "url": "https://github.com/DemoMacro/docen/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Demo Macro",
    "email": "abc@imst.xyz",
    "url": "https://imst.xyz/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DemoMacro/docen.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./docx": {
      "types": "./dist/docx.d.ts",
      "import": "./dist/docx.mjs",
      "require": "./dist/docx.cjs"
    }
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "docx": "9.5.1",
    "image-meta": "0.2.2",
    "ofetch": "1.5.1"
  },
  "devDependencies": {
    "@docen/extensions": "workspace:*",
    "@docen/utils": "workspace:*",
    "@tiptap/core": "3.16.0"
  }
}
